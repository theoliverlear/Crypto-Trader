{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "crypto-trader": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "src/main/resources/templates/angular_build",
            "index": "angular/index.html",
            "main": "angular/main.ts",
            "polyfills": "angular/polyfills.ts",
            "tsConfig": "tsconfig.json",
            "aot": false,
            "assets": [
              {
                "glob": "**/*",
                "input": "angular/assets",
                "output": "/assets/"
              }
            ],
            "styles": [
              "angular/components/app/app.component.css",
              "angular/components/elements/content-stripe/content-stripe.component.css",
              "angular/components/elements/element-group-auth/auth-console-login-section/auth-console-login-section.component.css",
              "angular/components/elements/element-group-auth/auth-console-signup-section/auth-console-signup-section.component.css",
              "angular/components/elements/element-group-auth/auth-console-tab-section/auth-console-tab-section.component.css",
              "angular/components/elements/element-group-auth/auth-console-tab/auth-console-tab.component.css",
              "angular/components/elements/element-group-auth/auth-console/auth-console.component.css",
              "angular/components/elements/element-group-auth/auth-input/auth-input.component.css",
              "angular/components/elements/element-group-chart/chart/chart.component.css",
              "angular/components/elements/element-group-chart/live-chart/live-chart.component.css",
              "angular/components/elements/element-group-native/ss-anchor/ss-anchor.component.css",
              "angular/components/elements/element-group-native/ss-button/ss-button.component.css",
              "angular/components/elements/element-group-native/ss-footer/ss-footer.component.css",
              "angular/components/elements/element-group-native/ss-img/ss-img.component.css",
              "angular/components/elements/element-group-native/ss-input/ss-input.component.css",
              "angular/components/elements/element-group-native/ss-title/ss-title.component.css",
              "angular/components/elements/element-group-nav/nav-bar-item/nav-bar-item.component.css",
              "angular/components/elements/element-group-nav/nav-bar-profile-picture/nav-bar-profile-picture.component.css",
              "angular/components/elements/element-group-nav/nav-bar/nav-bar.component.css",
              "angular/components/elements/element-group-portfolio/currency-wallet/currency-wallet.component.css",
              "angular/components/elements/element-group-portfolio/element-group-performance/performance-chart-settings/performance-chart-settings.component.css",
              "angular/components/elements/element-group-portfolio/element-group-performance/performance-chart/performance-chart.component.css",
              "angular/components/elements/element-group-portfolio/element-group-performance/performance-section/performance-section.component.css",
              "angular/components/elements/element-group-portfolio/portfolio-input/portfolio-input.component.css",
              "angular/components/elements/element-group-portfolio/portfolio-section-selector-bubble/portfolio-section-selector-bubble.component.css",
              "angular/components/elements/element-group-portfolio/portfolio-section-selector/portfolio-section-selector.component.css",
              "angular/components/elements/element-group-portfolio/portfolio-section/portfolio-section.component.css",
              "angular/components/elements/element-group-profile/profile-picture/profile-picture.component.css",
              "angular/components/elements/element-group-text/subtitle/subtitle.component.css",
              "angular/components/elements/element-group-trader/trader-event-bubble/trader-event-bubble.component.css",
              "angular/components/elements/popup/popup.component.css",
              "angular/components/pages/account/account.component.css",
              "angular/components/pages/authorize/authorize.component.css",
              "angular/components/pages/currencies/currencies.component.css",
              "angular/components/pages/home/home.component.css",
              "angular/components/pages/not-found/not-found.component.css",
              "angular/components/pages/portfolio/portfolio.component.css",
              "angular/components/pages/terms-of-service/terms-of-service.component.css",
              "angular/components/pages/trader/trader.component.css"
            ],
            "scripts": [],
            "fileReplacements": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "angular/environments/environment.ts",
                  "with": "angular/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "none",
              "optimization": true,
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "development": {
              "fileReplacements": [
                {
                  "replace": "angular/environments/environment.ts",
                  "with": "angular/environments/environment.dev.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "none",
              "sourceMap": true,
              "namedChunks": true,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "crypto-trader:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "crypto-trader:build:production"
            },
            "development": {
              "buildTarget": "crypto-trader:build:development"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "crypto-trader:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "angular/test.ts",
            "polyfills": "angular/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "angular/assets"
            ],
            "styles": [
              "angular/styles/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "crypto-trader:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "crypto-trader:serve:production"
            },
            "development": {
              "devServerTarget": "crypto-trader:serve:development"
            }
          }
        }
      }
    }
  }
}